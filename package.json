{
  "name": "coworking-platform",
  "version": "0.1.0",
  "private": true,
  "description": "Plateforme de coworking unifiÃ©e avec Next.js 14",
  "packageManager": "pnpm@10.10.0",
  "scripts": {
    "dev": "next dev",
    "dev:start": "next dev",
    "dev:socketio": "node scripts/start-socketio.js",
    "dev:manual": "npm --loglevel=error run dev:start",
    "dev:verbose": "next dev",
    "build": "SKIP_TYPE_CHECK=true next build",
    "build:deploy": "tsc --project tsconfig.deploy.json --noEmit && next build",
    "build:loose": "SKIP_TYPE_CHECK=true next build",
    "start": "node server.js",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:mobile": "vitest --config vitest.config.ts --reporter=verbose --run __tests__/components/dashboard/mobile/",
    "test:mobile:watch": "vitest --config vitest.config.ts --watch __tests__/components/dashboard/mobile/",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:mobile": "cypress run --spec 'cypress/e2e/mobile/**/*'",
    "cypress:mobile:open": "cypress open --e2e --config specPattern='cypress/e2e/mobile/**/*'",
    "test:e2e": "cypress run",
    "test:e2e:mobile": "npm run cypress:mobile",
    "test:all": "npm run test && npm run test:e2e",
    "test:all:mobile": "npm run test:mobile && npm run test:e2e:mobile",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,md,json}\"",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "tsx scripts/seed.ts",
    "db:init-spaces": "tsx scripts/init-spaces.ts",
    "clear-auth-cache": "node scripts/clear-auth-cache.js",
    "init-messaging": "node scripts/init-messaging.js"
  },
